CREATE TABLE Patient(
aadhar_id char(12),
First_name varchar(30) NOT NULL,
Last_name  varchar(20) NOT NULL, 
Age int NOT NULL,
Sex varchar(1) NOT NULL,
address varchar(50) ,
Mobile char(10) NOT NULL,
PRIMARY KEY (aadhar_id),
INDEX (aadhar_id)
);
CREATE TABLE Appointment(
appointment_id char(10) NOT NULL,
ADate DATE   NOT NULL,
ATime varchar(4) NOT NULL,
Cid char(12) NOT NULL,
FOREIGN KEY(Cid) REFERENCES Patient(aadhar_id),
INDEX (appointment_id),
PRIMARY KEY (appointment_id)
);

Last_name  varchar(20) NOT NULL, 
Age int NOT NULL,
Sex varchar(1) NOT NULL,
address varchar(50) ,
Mobile char(10) NOT NULL,
PRIMARY KEY (aadhar_id),
INDEX (aadhar_id)
);
