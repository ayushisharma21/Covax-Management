INSERT INTO Appointment VALUES(1212133,'2021-03-03','12:20:33',230379894564);
CREATE TABLE VaccineDose(
Vnumber int NOT NULL,
appointment_id char(10) NOT NULL,
Patient_id     char(12)   NOT NULL,
Vaccine_name varchar(20) NOT NULL,
FOREIGN KEY(appointment_id) REFERENCES Appointment(appointment_id),
FOREIGN KEY(Patient_id) REFERENCES Patient(aadhar_id),
INDEX (Vnumber,Vaccine_name),
PRIMARY KEY (Vnumber,Vaccine_name)
);
CREATE TABLE SUPPLIERS(
Supplier_regNO int NOT NULL,
Sname varchar(30) NOT NULL,
SAddress varchar(30) NOT NULL,
Telephone_number char(10) NOT NULL,
INDEX(Supplier_regNO),
PRIMARY KEY (Supplier_regNO)
);

CREATE TABLE STOCK(
Bcode int NOT NULL,
Vaccine_name varchar(30),
Locality varchar(30) NOT NULL,
Area varchar(30) NOT NULL,
Quantity_departed int NOT NULL,
Quantity_Remaning int NOT NULL,
Supplier_regNo int NOT NULL,
INDEX(Area,Locality),
INDEX(Bcode),
PRIMARY KEY(Area,Locality),
FOREIGN KEY(Supplier_regNo) REFERENCES SUPPLIERS(Supplier_regNO)
);






